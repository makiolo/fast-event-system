{
  "name": "fast-event-system",
  "version": "1.0.11",
  "description": "This fast event system allows calls between two interfaces decoupled (sync or async)",
  "npm-mas-mas": {
	  "dependencies": {
		  "boost-atomic": "1.65.0.0",
		  "boost-chrono": "1.65.0.0",
		  "boost-context": "1.65.0.0",
		  "boost-coroutine2": "1.65.0.0",
		  "boost-filesystem": "1.65.0.0",
		  "boost-headers": "1.65.0.0",
		  "boost-system": "1.65.0.0",
		  "boost-thread": "1.65.0.0",
		  "google-gmock": "0.0.1468494938.1354555194"
	  },
	  "install": [
		  "./*.h include/fast-event-system/",
		  "./concurrentqueue/*.h include/fast-event-system/concurrentqueue/"
	  ],
	  # CXXFLAGS
	  # LDFLAGS
	  # INCLUDES
	  # DEFINITIONS
	  "targets": {
		  "public": [
			  {
				  # CXXFLAGS
				  # LDFLAGS
				  # INCLUDES
				  # DEFINITIONS
