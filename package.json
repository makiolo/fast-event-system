			  }
		  ],
		  "private": [
			  {
				  "gtest": "test_fes tests/test_fes.cpp DEPENDS fes"
			  },
			  {
				  "gtest": "test_sync tests/test_sync.cpp DEPENDS fes"
			  },
			  {
				  "gtest": "test_async_fast tests/test_async_fast.cpp DEPENDS fes"
			  },
			  {
				  "gtest": "test_async_fast_autoconnect tests/test_async_fast_autoconnect.cpp DEPENDS fes"
			  },
			  {
				  "gtest": "test_async_delay tests/test_async_delay.cpp DEPENDS fes"
			  }
		  ]
	  }
  },
  "scripts": {
    "ci": "curl -s https://raw.githubusercontent.com/makiolo/cmaki_scripts/master/ci.sh | bash",
